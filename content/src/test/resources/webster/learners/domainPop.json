    "definitions":[
      {
        "senseSequence":[
          [
            {
              "definingText":{
                "text":[
                  "to suddenly break open or come away from something often with a short, loud noise"
                ],
                "examples":[
                  {
                    "text":"The balloon *popped*."
                  },
                  {
                    "text":"We heard the sound of corks *popping* as the celebration began."
                  },
                  {
                    "text":"One of the buttons *popped* off my sweater."
                  },
                  {
                    "text":"Don't *pop* that balloon!"
                  },
                  {
                    "text":"She *popped* the cork on the champagne."
                  }
                ]
              }
            },
            {
              "definingText":{
                "text":[
                  "to make a short, loud noise"
                ],
                "examples":[
                  {
                    "text":"Guns were *popping* in the distance."
                  }
                ]
              },
              "senseSpecificGrammaticalLabel":"no obj"
            }
          ],
          [
            {
              "definingText":{
                "text":[
                  "to cook (popcorn)"
                ],
                "examples":[
                  {
                    "text":"We *popped* some popcorn in the microwave."
                  },
                  {
                    "text":"The popcorn is done *popping*."
                  }
                ]
              }
            }
          ],
          [
            {
              "definingText":{
                "text":[
                  "to come from, into, or out of a place suddenly or briefly"
                ],
                "examples":[
                  {
                    "text":"I didn't mean to say that—it just *popped* out."
                  },
                  {
                    "text":"Her shoulder *popped* out of its socket."
                  },
                  {
                    "text":"He opened the box, and out *popped* a mouse."
                  },
                  {
                    "text":"A funny thought just *popped* into my head."
                  },
                  {
                    "text":"The cathedral suddenly *popped* into view."
                  },
                  {
                    "text":"Her father *pops* in and out of her life."
                  }
                ]
              }
            },
            {
              "definingText":{
                "text":[
                  "to go to or from a place quickly, suddenly, or briefly"
                ],
                "examples":[
                  {
                    "text":"If you are busy, I can *pop* back in later."
                  },
                  {
                    "text":"She *popped* over for a cup of tea. = (*Brit*) She *popped* round for a cup of tea."
                  },
                  {
                    "text":"My neighbor *popped* in for a visit."
                  },
                  {
                    "text":"I need to *pop* into the drugstore for some film."
                  },
                  {
                    "text":"She *popped* out for a minute. She should be back soon."
                  },
                  {
                    "text":"I'll *pop* down to the post office during my break."
                  }
                ]
              },
              "labels":[
                "always followed by an adverb or preposition"
              ]
            }
          ],
          [
            {
              "definingText":{
                "text":[
                  "to put (something) in, into, or onto a place suddenly or briefly"
                ],
                "examples":[
                  {
                    "text":"She *popped* a CD in the player."
                  },
                  {
                    "text":"He *popped* a quarter in the jukebox."
                  },
                  {
                    "text":"I *popped* a grape into my mouth."
                  },
                  {
                    "text":"He *popped* his head out the window."
                  }
                ]
              },
              "labels":[
                "always followed by an adverb or preposition"
              ],
              "senseSpecificGrammaticalLabel":"+ obj",
              "subjectStatusLabels":[
                "informal"
              ]
            }
          ],
          [
            {
              "definingText":{
                "text":[
                  "to hit (someone)"
                ],
                "examples":[
